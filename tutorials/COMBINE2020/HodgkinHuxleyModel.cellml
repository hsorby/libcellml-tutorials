<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.cellml.org/cellml/2.0#" name="HodgkinHuxleyModel">
  <import xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="MembraneModel.cellml">
    <component component_ref="membrane" name="membrane"/>
  </import>
  <import xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="Controller.cellml">
    <component component_ref="controller" name="controller"/>
  </import>
  <import xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="Parameters.cellml">
    <component component_ref="parameters" name="parameters"/>
  </import>
  <connection component_1="membrane" component_2="parameters">
    <map_variables variable_1="V" variable_2="V"/>
    <map_variables variable_1="Cm" variable_2="Cm"/>
    <map_variables variable_1="h" variable_2="h"/>
    <map_variables variable_1="m" variable_2="m"/>
    <map_variables variable_1="E_Na" variable_2="E_Na"/>
    <map_variables variable_1="g_Na" variable_2="g_Na"/>
    <map_variables variable_1="n" variable_2="n"/>
    <map_variables variable_1="E_K" variable_2="E_K"/>
    <map_variables variable_1="g_K" variable_2="g_K"/>
    <map_variables variable_1="E_L" variable_2="E_L"/>
    <map_variables variable_1="g_L" variable_2="g_L"/>
  </connection>
  <connection component_1="membrane" component_2="controller">
    <map_variables variable_1="i_stim" variable_2="i_stim"/>
  </connection>
</model>
